dist = [
    [0, 310, 716, 408, 852],
    [310, 0, 470, 705, 1144],
    [716, 470, 0, 1119, 1553],
    [408, 705, 1119, 0, 429],
    [852, 1144, 1553, 429, 0]
]

cidades = ["Curitiba", "Florianópolis", "Porto Alegre", "São Paulo", "Rio de Janeiro"]

for a, c in enumerate(cidades):
    print(f"{a} - {c}")

o = int(input("Origem: "))
d = int(input("Destino: "))

print(f"Distância: {dist[o][d]} km")
